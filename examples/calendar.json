{
  "aid": "aid://productivity/calendar@1.2.0",
  "name": "Calendar Pro - Intelligent Scheduling Agent",
  "description": "Advanced calendar management and intelligent scheduling assistant with natural language processing, conflict detection, and multi-timezone support for professional teams",
  "owner": {
    "org": "Productivity Systems Inc",
    "site": "https://calendarpro.io",
    "contact": "api@calendarpro.io"
  },
  "capabilities": [
    {
      "id": "calendar.create_event",
      "description": "Create calendar events with automatic conflict detection and optimal time slot suggestions",
      "inputs": {
        "title": "string",
        "description": "string",
        "start_time": "ISO8601 datetime",
        "end_time": "ISO8601 datetime",
        "attendees": "array of email addresses",
        "timezone": "IANA timezone identifier"
      },
      "outputs": {
        "event_id": "string",
        "status": "created | conflict | suggested_alternatives",
        "conflicts": "array of conflicting events",
        "suggested_times": "array of alternative time slots"
      }
    },
    {
      "id": "calendar.find_availability",
      "description": "Find optimal meeting times across multiple participants and time zones",
      "inputs": {
        "participants": "array of user identifiers",
        "duration_minutes": "integer",
        "date_range": "object with start and end dates",
        "preferences": "object with time preferences"
      },
      "outputs": {
        "available_slots": "array of time slots with availability scores",
        "recommendations": "top 3 recommended times with reasoning"
      }
    },
    {
      "id": "calendar.sync_external",
      "description": "Synchronize with Google Calendar, Outlook, and other calendar systems",
      "inputs": {
        "provider": "google | outlook | apple | ical",
        "credentials": "OAuth token or API key",
        "sync_direction": "one_way | two_way"
      },
      "outputs": {
        "sync_status": "success | partial | failed",
        "events_synced": "integer",
        "errors": "array of sync errors"
      }
    },
    {
      "id": "calendar.get_schedule",
      "description": "Retrieve schedule with smart filtering and summarization",
      "inputs": {
        "user_id": "string",
        "start_date": "ISO8601 date",
        "end_date": "ISO8601 date",
        "filters": "object with event type, attendee, and other filters"
      },
      "outputs": {
        "events": "array of calendar events",
        "summary": "text summary of key events and commitments"
      }
    }
  ],
